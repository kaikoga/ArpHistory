<?xml version="1.0" encoding="utf-8"?>
<extension>

	<section if="arp_backend_flash">
		<set name="arp_backend" value="flash" />
	</section>

	<section if="arp_backend_openfl">
		<set name="arp_backend" value="openfl" />
	</section>

	<section if="arp_backend_kha">
		<set name="arp_backend" value="kha" />
	</section>

	<section unless="arp_backend">
		<section if="flash">
			<set name="arp_backend_flash" value="1" />
		</section>
	
		<section unless="flash">
			<set name="arp_backend_openfl" value="1" />
		</section>
	</section>

</extension>
